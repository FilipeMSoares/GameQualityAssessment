                        <td class="coluna-o faixa-1" rel="grafico-posicao"><div class="">1</div></td>
                        <td class="time">São Paulo</td>
                        <td class="coluna-p" rel="jogos-pontos"><div class="">33</div></td>
                        <td class="coluna-j" rel="jogos-jogos"><div class="">16</div></td>
