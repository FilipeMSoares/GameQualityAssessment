<script>
	$(document).ready(function(){
        initRodadas();
        var totalRodadas = $('.tabela-classificacao .tabela').length;

        $('.pontos-corridos .navegador-rodada .navegador .anterior').bind( 'click', navegaProximoAnterior );
        $('.pontos-corridos .navegador-rodada .navegador .proximo').bind( 'click', navegaProximoAnterior );

        $(".pontos-corridos .navegador-rodada .navegador .rodada").bind("click", function(e){
            var container = $(".lista-rodadas", $(e.target).parents(".rodada"));
            container.toggle();
            if($.browser.msie){
                if(container.css('display') == 'block'){
                    $('.tabela-classificacao table').css('z-index', '-1');
                }else{
                    $('.tabela-classificacao table').css('z-index', '0');
                }
            }
        });

        var opt = {
            campeonato: "campeonato-brasileiro",
            edicao: "brasileirao2013",
            fase: $('.rodada-tabela .tabela').attr("data-fase"),
            exibirDadosScouts: 'true'.toLowerCase() == 'true'
        }
        $.extend(opt, hashAtual);
        classificacaoAprofundamento.init(opt);

        $(".pontos-corridos .navegador-rodada .navegador .rodada .lista-rodadas li").bind("click", navegaRodadaEspecifica );

        $(".pontos-corridos .lista-rodadas").each(function(index, item){
            ultimo_li = $("ul li", item).last();
            navegaRodadaEspecifica({ target: ultimo_li }, {"naoMudarHash" : true});
        });

	});
</script>

