                    <a href="/campeonato/campeonato-brasileiro/2008/07/12/goias-2-x-2-coritiba">
                      <meta itemprop="name" content='Goiás 2 x
                      2 Coritiba' />
                      <link itemprop="url" href="http://futpedia.globo.com/campeonato/campeonato-brasileiro/2008/07/12/goias-2-x-2-coritiba"/>
                      <time itemprop="startDate" datetime='12/07/2008' />
                	    <div class="data-local" itemprop="location" itemscope itemtype="http://schema.org/StadiumOrArena">
                		    Sab
                		    12/07/2008 -
                		    <span class="horario">18h20</span>
                		    <span itemprop="name">Serra Dourada</span>
                	    </div>
                	    <div class="info-jogo">
                		    <div class="time mandante" itemprop="performers" itemscope itemtype="http://schema.org/SportsTeam">
                			    <div class="jogo-escudo-mandante"><img src="http://s.glbimg.com/es/sde/f/equipes/2013/12/16/goias_30x30.png"></div>
                			    <span class="nome font-face">GOI</span>
                              <meta itemprop="name" content="Goiás" />
                		    </div>
