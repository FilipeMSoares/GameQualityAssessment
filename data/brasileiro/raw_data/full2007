                      <meta itemprop="name" content='Botafogo 2 x
                      3 Corinthians' />
                      <link itemprop="url" href="http://futpedia.globo.com/campeonato/campeonato-brasileiro/2007/08/15/botafogo-2-x-3-corinthians"/>
                      <time itemprop="startDate" datetime='15/08/2007' />
                	    <div class="data-local" itemprop="location" itemscope itemtype="http://schema.org/StadiumOrArena">
                		    Qua
                		    15/08/2007 -
                		    <span class="horario">20h30</span>
                		    <span itemprop="name">Maracanã</span>
                	    </div>
