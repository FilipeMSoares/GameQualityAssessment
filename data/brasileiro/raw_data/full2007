                    <tr class="linha-classificacao" rel="sao-paulo" data-escudo="http://s.glbimg.com/es/sde/f/equipes/2013/12/16/sao_paulo_30x30.png">
                        <td class="coluna-o faixa-1" rel="grafico-posicao"><div class="">1</div></td>
                        <td class="time">São Paulo</td>
                        <td class="coluna-p" rel="jogos-pontos"><div class="">50</div></td>
