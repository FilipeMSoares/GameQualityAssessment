                                <li class="lista-classificacao-jogo" data-rodada="31" data-fase="2322" itemprop="subEvents" itemscope itemtype="http://schema.org/SportsEvent/Match">
                    <a href="/campeonato/campeonato-brasileiro/2010/10/23/botafogo-1-x-0-vitoria">
                      <meta itemprop="name" content='Botafogo 1 x
                      0 Vitória' />
                      <link itemprop="url" href="http://futpedia.globo.com/campeonato/campeonato-brasileiro/2010/10/23/botafogo-1-x-0-vitoria"/>
                      <time itemprop="startDate" datetime='23/10/2010' />
                	    <div class="data-local" itemprop="location" itemscope itemtype="http://schema.org/StadiumOrArena">
                		    Sab
                		    23/10/2010 -
                		    <span class="horario">18h30</span>
                		    <span itemprop="name">Engenhão</span>
                	    </div>
                	    <div class="info-jogo">
                		    <div class="time mandante" itemprop="performers" itemscope itemtype="http://schema.org/SportsTeam">
                			    <div class="jogo-escudo-mandante"><img src="http://s.glbimg.com/es/sde/f/equipes/2013/12/16/botafogo_30x30.png"></div>
                			    <span class="nome font-face">BOT</span>
                              <meta itemprop="name" content="Botafogo" />
                		    </div>
                		    <div class="placar">
