                    <a href="/campeonato/campeonato-brasileiro/2010/08/14/atletico-mg-3-x-1-guarani">
                      <meta itemprop="name" content='Atlético-MG 3 x
                      1 Guarani' />
                      <link itemprop="url" href="http://futpedia.globo.com/campeonato/campeonato-brasileiro/2010/08/14/atletico-mg-3-x-1-guarani"/>
                      <time itemprop="startDate" datetime='14/08/2010' />
                	    <div class="data-local" itemprop="location" itemscope itemtype="http://schema.org/StadiumOrArena">
                		    Sab
                		    14/08/2010 -
                		    <span class="horario">18h30</span>
                		    <span itemprop="name">Ipatingão</span>
                	    </div>
                	    <div class="info-jogo">
                		    <div class="time mandante" itemprop="performers" itemscope itemtype="http://schema.org/SportsTeam">
                			    <div class="jogo-escudo-mandante"><img src="http://s.glbimg.com/es/sde/f/equipes/2013/12/16/atletico_mg_30x30.png"></div>
                			    <span class="nome font-face">CAM</span>
                              <meta itemprop="name" content="Atlético-MG" />
                		    </div>
